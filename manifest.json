{
  "manifest_version": 2,
  "name": "Booking Lite",
  "short_name": "Booking Lite",
  "version": "1.0.0",
  "description": "Makes booking less aggressive for your anxiety",
  "icons": {
    "16": "./assets/icon16.png",
    "48": "./assets/icon48.png",
    "128": "./assets/icon128.png"
  },
  "content_scripts": [
    {
      "css": ["styles.css"],
      "matches": ["https://booking.com/*", "https://www.booking.com/*"]
    }
  ]
}
